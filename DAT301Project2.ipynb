    "import pandas as pd \n",
    "\n",
    "inflationData = pd.read_csv(\"global_inflation_data.csv\",sep=\",\") #you need to include the .csv file in the same directory as this Jupyter notebook for this to work \n",
    "\n",
    "inflationDF = pd.DataFrame(inflationData)\n",
    "\n",
    "#INSERT MORE COUNTRIES \n",
    "USInflation = inflationDF.loc[inflationDF['country_name'] == \"United States\", \"2000\":\"2024\"]\n",
    "CanadaInflation = inflationDF.loc[inflationDF['country_name'] == \"Canada\", \"2000\":\"2024\"] \n",
    "ChinaInflation = inflationDF.loc[inflationDF['country_name'] == \"China, People's Republic of\", \"2000\":\"2024\"]\n",
    "IndiaInflation = inflationDF.loc[inflationDF['country_name'] == \"India\", \"2000\":\"2024\"]\n",
    "MexicoInflation = inflationDF.loc[inflationDF['country_name'] == \"Mexico\", \"2000\":\"2024\"]\n",
    "RussiaInflation = inflationDF.loc[inflationDF['country_name'] == \"Russian Federation\", \"2000\":\"2024\"]\n",
    "\n",
    "#INSERT MORE COUNTRIES \n",
    "\n",
    "#MAX Rates \n",
    "USMaxInflation = USInflation.max()\n",
    "CanadaMaxInflation = CanadaInflation.max()\n",
    "IndiaMaxInflation = IndiaInflation.max()\n",
    "ChinaMaxInflation = ChinaInflation.max()\n",
    "MexicoMaxInflation = MexicoInflation.max()\n",
    "RussiaMaxInflation = RussiaInflation.max()\n",
    "# Create a vector of maximum inflation rates\n",
    "max_inflation_rates = np.array([USMaxInflation, CanadaMaxInflation, ChinaMaxInflation, IndiaMaxInflation, MexicoMaxInflation, RussiaMaxInflation])\n",
    "\n",
    "# Create a dataframe of all maximum inflation rates\n", 
    "max_inflation_df = pd.DataFrame({'country': ['USA', 'Canada', 'China', 'India', 'Mexico', 'Russia'], 'rates': max_inflation_rates})\n",
    "\n",
    "#MIN Rates \n",
    "USMinInflation = USInflation.min()\n",
    "CanadaMinInflation = CanadaInflation.min()\n",
    "IndiaMinInflation = IndiaInflation.min()\n",
    "ChinaMinInflation = ChinaInflation.min()\n",
    "MexicoMinInflation = MexicoInflation.min()\n",
    "RussiaMinInflation = RussiaInflation.min()\n",
    "min_inflation_rates = np.array([USminInflation, CanadaMinInflation, ChinaMinInflation, IndiaMinInflation, MexicoMinInflation, RussiaMinInflation])\n",
    "min_inflation_df = pd.DataFrame({'country': ['USA', 'Canada', 'China', 'India', 'Mexico', 'Russia'], 'rates': min_inflation_rates})\n",
    "\n",
    "#MEAN Rates \n",
    "USMeanInflation = USInflation.mean()\n",
    "CanadaMeanInflation = CanadaInflation.mean()\n",
    "IndiaMeanInflation = IndiaInflation.mean()\n",
    "ChinaMeanInflation = ChinaInflation.mean()\n",
    "MexicoMeanInflation = MexicoInflation.mean()\n",
    "RussiaMeanInflation = RussiaInflation.mean()\n",
    "\n",
    "mean_inflation_rates = np.array([USMeanInflation, CanadaMeanInflation, ChinaMeanInflation, IndiaMeanInflation, MexicoMeanInflation, RussiaMeanInflation])\n",
    "mean_df = pd.DataFrame({'country': ['USA', 'Canada', 'China', 'India', 'Mexico', 'Russia'], 'means': mean_inflation_rates})\n",
    "\n",
    "#STD_DEV Rates \n",
    "USMaxInflation = USInflation.std()\n",
    "CanadaMaxInflation = CanadaInflation.std()\n",
    "IndiaMaxInflation = IndiaInflation.std()\n",
    "ChinaMaxInflation = ChinaInflation.std()\n",
    "MexicoMaxInflation = MexicoInflation.std()\n",
    "RussiaMaxInflation = RussiaInflation.std()\n",
    "\n",
    "inflation_std_devs = np.array([USAsd, CanadaSD, ChinaSD, IndiaSD, MexicoSD, RussiaSD])\n",
    "std_dev_df = pd.DataFrame({'country': ['USA', 'Canada', 'China', 'India', 'Mexico', 'Russia'], 'stdDev': inflation_std_devs})\n",
    "\n",
    "\n",
    "\n",
    "#Graphs \n",
    "# MAX Inflation Graph\n",
    "plt.figure(figsize=(10, 6))\n",
    "max_inflation_df.plot(kind='bar', x='country', y='rates', rot=0)\n"
    "plt.title('Maximum Inflation Rates')\n",
    "plt.xlabel('Country')\n",
    "plt.ylabel('Inflation Rate')\n",
    "plt.show()\n",
    "\n",
    "# MIN Inflation Graph\n",
    "plt.figure(figsize=(10, 6))\n",
    "max_inflation_df.plot(kind='bar', x='country', y='rates', rot=0)\n"
    "plt.title('Minimum Inflation Rates')\n",
    "plt.xlabel('Country')\n",
    "plt.ylabel('Inflation Rate')\n",
    "plt.show()\n",
    "\n",
    "# MEAN Inflation Graph\n",
    "plt.figure(figsize=(10, 6))\n",
    "mean_df.plot(kind='bar', x='country', y='means', rot=65, width=0.5, color='tomato3')\n",
    "plt.title('Country vs. Means')\n",
    "plt.xlabel('Country')\n",
    "plt.ylabel('Inflation Rate')\n",
    "plt.show()\n",
    "\n",
    "# STD DEV Inflation Graph\n",
    "plt.figure(figsize=(10, 6))\n",
    "std_dev_df.plot(kind='bar', x='country', y='stdDev', rot=0)\n",
    "plt.title('Inflation Standard Deviations')\n",
    "plt.xlabel('Country')\n",
    "plt.ylabel('Standard Deviation')\n",
    "for i, v in enumerate(inflation_std_devs):\n",
    "  plt.text(i, v, str(round(v, 3)), ha='center', va='bottom')\n",
    "plt.show()\n",
    "\n"
